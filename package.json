  "devDependencies": {
    "@types/node": "^12.12.25",
    "serverless": "^1.63.0",
    "serverless-localstack": "^0.4.24",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
